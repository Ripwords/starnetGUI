        "copyFile": true
      },
      "shell": {
        "all": true
      },
      "dialog": {
        "open": true
      },
      "os": {
        "all": true
      },
      "path": {
        "all": true
      },
      "window": {
        "setTitle": true
      }
    },
    "windows": [
      {
        "title": "StarNet++",
        "width": 500,
        "height": 600,
        "resizable": true,
        "fullscreen": false,
        "fileDropEnabled": true
      }
    ],
    "security": {
      "csp": "default-src blob: data: filesystem: ws: wss: http: https: tauri: 'unsafe-eval' 'unsafe-inline' 'self' img-src: 'self'"
    }
